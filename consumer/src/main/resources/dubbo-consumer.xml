<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-consumer" version="1.0.0.local" logger="slf4j"/>
    <dubbo:registry id="zookeeper" protocol="zookeeper" address="zookeeper://127.0.0.1:2181"
                    file="/home/admin/dubbo-consumer/dubbo-registry.properties"/>
    <dubbo:protocol name="dubbo" port="-1"/>
    <dubbo:consumer check="false" timeout="5000" retries="2" actives="0" loadbalance="random" cluster="failover"
                    cache="lru" version="1.0.0.local"/>
    <dubbo:annotation package="org.dubbo.service"/>
    <dubbo:monitor protocol="dubbo" address="127.0.0.1:10881"/>


</beans>